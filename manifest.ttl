PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX mrr: <https://prez.dev/ManifestResourceRoles/>
PREFIX prez: <https://prez.dev/>
PREFIX prof: <http://www.w3.org/ns/dx/prof/>
PREFIX schema: <https://schema.org/>

[]
    a prez:Manifest ;
    prof:hasResource
        [
            prof:hasArtifact "resources/catalogues/*.ttl" ;
            prof:hasRole mrr:CatalogueData ;
            schema:description "The definition of, and medata for, the codelist vocabularies container object - a dcat:Catalog" ;
            schema:name "Catalogue Definition"
        ] ,
        [
            prof:hasArtifact "resources/codelists/**/*.ttl" ;
            prof:hasRole mrr:ResourceData ;
            schema:description "vocabularies in the vocabs/ folder and its children" ;
            schema:name "Resource Data" ;
            dcterms:conformsTo "validator-vocpub-iso.ttl" ;
        ] ,
        [
            prof:hasArtifact "resources/background/*.ttl" ;
            prof:hasRole mrr:CompleteCatalogueAndResourceLabels ;
            schema:description "RDF files containing labels for the resource data" ;
            schema:name "Labels Files" ;
        ] ;
.