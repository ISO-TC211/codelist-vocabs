:1
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "No change has been made to the specification"@en ;
    skos:historyNote "From page 52, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "No change"@en ;
.

:2
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The style of the specification has been changed to match the style and structure of other specifications in the register that has imported the specification"@en ;
    skos:historyNote "From page 52, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Style"@en ;
.

:3
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The specification includes information about its context that is not explicit in the specification in the external source"@en ;
    skos:historyNote "From page 52, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Extra not explict"@en ;
.

:4
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The specification of the register item has been generalized to have a broader meaning than the item specified in the external source"@en ;
    skos:historyNote "From page 52, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Generalized"@en ;
.

:5
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The specification of the register item has been specialized to have a narrower meaning than the item specified in the external source"@en ;
    skos:historyNote "From page 52, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Narrower"@en ;
.

:6
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The nature of the differences between the register item and the similar item in the external source is unspecified"@en ;
    skos:historyNote "From page 52, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Unspecified"@en ;
.

