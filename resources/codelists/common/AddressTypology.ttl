PREFIX : <https://def.isotc211.org/common/AddressTypology/>
PREFIX cs: <https://def.isotc211.org/common/AddressTypology>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX status: <https://def.isotc211.org/RE/ItemStatus/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:area
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The address class is based on a division of land or water into demarcated areas, such as neighbourhoods or precincts or cadastral features." ;
    skos:historyNote "Presented in the original standard's codelist"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "area"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:service
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The address class is based on delivery or collection services, such as a group of post boxes at the same location or poste restante." ;
    skos:historyNote "Presented in the original standard's codelist"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "service"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:thoroughfare
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The address class is based on navigable access features, such as streets or canals." ;
    skos:historyNote "Presented in the original standard's codelist"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "thoroughfare"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

cs:
    a skos:ConceptScheme ;
    rdfs:isDefinedBy <https://def.isotc211.org/common/standard/iso-19160-1-2015> ;
    skos:definition "AddressTypology contains values to specify the type of an address class"@en ;
    skos:hasTopConcept
        :area ,
        :service ,
        :thoroughfare ;
    skos:historyNote "This codelist vocabulary was extracted directly from the defining standard: ISO19160-1:2015"@en ;
    skos:prefLabel "Address Typology"@en ;
    schema:citation "http://standards.iso.org/iso/19160/-1/ed-1/en/"^^xsd:anyURI ;
    schema:contributor <https://def.isotc211.org/org/ogc> ;
    schema:creator <https://def.isotc211.org/org/tc211> ;
    schema:dateCreated "2015-12-15"^^xsd:date ;
    schema:dateIssued "2025-11-19"^^xsd:date ;
    schema:dateModified "2025-08-01"^^xsd:date ;
    schema:isPartOf <https://def.isotc211.org/common> ;
    schema:publisher <https://def.isotc211.org/org/tc211> ;
    schema:status status:stable ;
.
