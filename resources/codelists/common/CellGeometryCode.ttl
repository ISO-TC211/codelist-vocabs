PREFIX : <https://def.isotc211.org/common/CellGeometryCode/>
PREFIX cs: <https://def.isotc211.org/common/CellGeometryCode>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX status: <https://def.isotc211.org/RE/ItemStatus/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:area
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/CellGeometryCode> ;
    skos:definition "each cell represents an area"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "area"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:point
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/CellGeometryCode> ;
    skos:definition "each cell represents a point"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "point"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:stratum
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/CellGeometryCode> ;
    skos:definition "height range for a single point vertical profile"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "stratum"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:voxel
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/CellGeometryCode> ;
    skos:definition "each cell represents a volumetric measurement on a regular grid in three dimensional space"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "voxel"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

cs:
    a skos:ConceptScheme ;
    rdfs:isDefinedBy <https://def.isotc211.org/common/standard/iso-awi-19115-1> ;
    skos:definition "code indicating the geometry represented by the grid cell value"@en ;
    skos:hasTopConcept
        :area ,
        :point ,
        :stratum ,
        :voxel ;
    skos:historyNote "This version of this codeset was produced for the 2025 update of the 19115 standard" ;
    skos:prefLabel "Cell Geometry Code"@en ;
    schema:creator <https://def.isotc211.org/org/tc211> ;
    schema:dateCreated "2025-11-19"^^xsd:date ;
    schema:dateIssued "2025-11-19"^^xsd:date ;
    schema:dateModified "2025-11-19"^^xsd:date ;
    schema:isPartOf <https://def.isotc211.org/common> ;
    schema:publisher <https://def.isotc211.org/org/tc211> ;
    schema:status status:experimental ;
.
