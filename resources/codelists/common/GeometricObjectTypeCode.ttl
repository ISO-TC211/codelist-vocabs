PREFIX : <https://def.isotc211.org/common/GeometricObjectTypeCode/>
PREFIX cs: <https://def.isotc211.org/common/GeometricObjectTypeCode>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX status: <https://def.isotc211.org/RE/ItemStatus/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:complex
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/GeometricObjectTypeCode> ;
    skos:definition "set of geometric primitives such that their boundaries can be represented as a union of other primitives"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "complex"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:composite
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/GeometricObjectTypeCode> ;
    skos:definition "connected set of curves, solids or surfaces"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "composite"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:curve
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/GeometricObjectTypeCode> ;
    skos:definition "bounded, 1-dimensional geometric primitive, representing the continuous image of a line"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "curve"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:point
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/GeometricObjectTypeCode> ;
    skos:definition "zero-dimensional geometric primitive, representing a position but not having an extent"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "point"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:solid
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/GeometricObjectTypeCode> ;
    skos:definition "bounded, connected 3-dimensional geometric primitive, representing the continuous image of a region of space"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "solid"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:surface
    a skos:Concept ;
    rdfs:isDefinedBy <https://def.isotc211.org/MD/GeometricObjectTypeCode> ;
    skos:definition "bounded, connected 2-dimensional geometric primitive, representing the continuous image of a region of a plane"@en ;
    skos:historyNote "Originally presented in the ISO 19115-1:2014 standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "surface"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

cs:
    a skos:ConceptScheme ;
    rdfs:isDefinedBy <https://def.isotc211.org/common/standard/iso-awi-19115-1> ;
    skos:definition "name of point or vector objects used to locate zero-, one-, two-, or three-dimensional spatial locations in the dataset"@en ;
    skos:hasTopConcept
        :complex ,
        :composite ,
        :curve ,
        :point ,
        :solid ,
        :surface ;
    skos:historyNote "This version of this codeset was produced for the 2025 update of the 19115 standard" ;
    skos:prefLabel "Geometric Object Type Code"@en ;
    schema:creator <https://def.isotc211.org/org/tc211> ;
    schema:dateCreated "2025-11-19"^^xsd:date ;
    schema:dateIssued "2025-11-19"^^xsd:date ;
    schema:dateModified "2025-11-19"^^xsd:date ;
    schema:isPartOf <https://def.isotc211.org/common> ;
    schema:publisher <https://def.isotc211.org/org/tc211> ;
    schema:status status:experimental ;
.
