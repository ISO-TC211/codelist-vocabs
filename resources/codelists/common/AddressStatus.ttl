PREFIX : <https://def.isotc211.org/common/AddressStatus/>
PREFIX cs: <https://def.isotc211.org/common/AddressStatus>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX status: <https://def.isotc211.org/RE/ItemStatus/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:official
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "An official addressing authority assigned the address." ;
    skos:historyNote "Presented in the original standard's codelist"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "official"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:unknown
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The status of the address is unknown." ;
    skos:historyNote "Presented in the original standard's codelist"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "unknown"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:unofficial
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The address was not assigned by an official addressing authority." ;
    skos:historyNote "Presented in the original standard's codelist"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "unofficial"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

cs:
    a skos:ConceptScheme ;
    rdfs:isDefinedBy <https://def.isotc211.org/common/standard/iso-19160-1-2015> ;
    skos:definition "AddressStatus contains values to specify the status of an address"@en ;
    skos:hasTopConcept
        :official ,
        :unknown ,
        :unofficial ;
    skos:historyNote "This codelist vocabulary was extracted directly from the defining standard: ISO19160-1:2015"@en ;
    skos:prefLabel "Address Status"@en ;
    schema:citation "http://standards.iso.org/iso/19160/-1/ed-1/en/"^^xsd:anyURI ;
    schema:contributor <https://def.isotc211.org/org/ogc> ;
    schema:creator <https://def.isotc211.org/org/tc211> ;
    schema:dateCreated "2015-12-15"^^xsd:date ;
    schema:dateIssued "2025-11-19"^^xsd:date ;
    schema:dateModified "2025-08-01"^^xsd:date ;
    schema:isPartOf <https://def.isotc211.org/common> ;
    schema:publisher <https://def.isotc211.org/org/tc211> ;
    schema:status status:stable ;
.
