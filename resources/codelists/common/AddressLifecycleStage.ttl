    skos:inScheme cs: ;
    skos:prefLabel "retired"@en ;
    skos:topConceptOf cs: ;
    skos:historyNote "Presented in the original standard's codelist"@en ;
.

:unknown
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The lifecycle stage of the address or address component is unknown." ;
    skos:inScheme cs: ;
    skos:prefLabel "unknown"@en ;
    skos:topConceptOf cs: ;
    skos:historyNote "Presented in the original standard's codelist"@en ;
