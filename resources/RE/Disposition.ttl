PREFIX : <http://def.isotc211.org/RE/Disposition/>
PREFIX cs: <http://def.isotc211.org/RE/Disposition>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX status: <http://def.isotc211.org/RE/ItemStatus/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:accepted
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The control body decided to accept the proposal"@en ;
    skos:historyNote "From page 49, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Accepted"@en ;
.

:notAccepted
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The control body decided not to accept the proposal"@en ;
    skos:historyNote "From page 49, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Not Accepted"@en ;
.

:withdrawn
    a skos:Concept ;
    schema:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The submitting organization has withdrawn the proposal"@en ;
    skos:historyNote "From page 49, ISO19135-1:2014"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Withdrawn"@en ;
.

cs:
    a skos:ConceptScheme ;
    schema:status status:experimental ;
    skos:definition "Instances of RE_Disposition that identify the disposition of the proposal"@en ;
    skos:hasTopConcept
        :accepted ,
        :notAccepted ,
        :withdrawn ;
    skos:historyNote "Created manually by GOM staff directly from the ISO19132-1:2014 document"@en ;
    skos:prefLabel "Decision Status Codes"@en ;
    schema:citation "http://standards.iso.org/iso/19135/-1/ed-1/en/"^^xsd:anyURI ;
    schema:creator <https://committee.iso.org/home/tc211> ;
    schema:dateCreated "2014-09-18"^^xsd:date ;
    schema:dateModified "2025-08-01"^^xsd:date ;
    schema:publisher <https://committee.iso.org/home/tc211> ;
    schema:isPartOf <https://def.isotc211.org/codesets> ;
.
