PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX prof: <http://www.w3.org/ns/dx/prof/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

dcterms:conformsTo
    schema:description "An established standard to which the described resource conforms."@en ;
    schema:name "Conforms To"@en ;
.

dcterms:hasPart
    schema:description "A related resource that is included either physically or logically in the described resource."@en ;
    schema:name "Has Part"@en ;
.

dcterms:isPartOf
    schema:description "A related resource in which the described resource is physically or logically included."@en ;
    schema:name "Is Part Of"@en ;
.

rdfs:isDefinedBy
    schema:description "The defininition of the subject resource." ;
    schema:name "is defined by" ;
.

skos:Concept
    schema:description
        "An idea or notion; a unit of thought" ,
        "An idea or notion; a unit of thought"@en ;
    schema:name
        "Concept" ,
        "Concept"@en ;
.

skos:ConceptScheme
    schema:description "A set of concepts, optionally including statements about semantic relationships between those concepts"@en ;
    schema:name "Concept Scheme"@en ;
.

skos:altLabel
    schema:description "An alternative lexical label for a resource"@en ;
    schema:name "alternative label"@en ;
.

skos:broader
    schema:description "Relates a concept to a concept that is more general in meaning"@en ;
    schema:name "has broader"@en ;
.

skos:definition
    schema:description "A statement or formal explanation of the meaning of a concept"@en ;
    schema:name "definition"@en ;
.

skos:hasTopConcept
    schema:description "Relates, by convention, a concept scheme to a concept which is topmost in the broader/narrower concept hierarchies for that scheme, providing an entry point to these hierarchies"@en ;
    schema:name "has top concept"@en ;
.

skos:historyNote
    schema:description "A note about the past state/use/meaning of a concept"@en ;
    schema:name "history note"@en ;
.

skos:inScheme
    schema:description "Relates a resource (for example a concept) to a concept scheme in which it is included"@en ;
    schema:name "is in scheme"@en ;
.

skos:notation
    schema:description "A notation, also known as classification code, is a string of characters such as \"T58.5\" or \"303.4833\" used to uniquely identify a concept within the scope of a given concept scheme"@en ;
    schema:name "notation"@en ;
.

skos:prefLabel
    schema:description "The preferred lexical label for a resource, in a given language"@en ;
    schema:name "preferred label"@en ;
.

skos:scopeNote
    schema:description "A note that helps to clarify the meaning and/or the use of a concept"@en ;
    schema:name "scope note"@en ;
.

skos:topConceptOf
    schema:description "Relates a concept to the concept scheme that it is a top level concept of"@en ;
    schema:name "is top concept in scheme"@en ;
.

dcat:Catalog
    schema:description "A curated collection of metadata about resources (e.g., datasets and data services in the context of a data catalog)."@en ;
    schema:name "Catalog"@en ;
.

prof:hasArtifact
    schema:description "The URL of a downloadable file with particulars such as its format and role indicated by the Resource Descriptor"@en ;
    schema:name "has artifact" ;
.

prof:hasResource
    schema:description "A resource which describes the nature of an artifact and the role it plays in relation to the Profile"@en ;
    schema:name "has resource"@en ;
.

prof:hasRole
    schema:description "The function of an artifact described by a Resource Descriptor, such as specification, guidance etc."@en ;
    schema:name "has role" ;
.

<https://prez.dev/Manifest>
    schema:name "Manifest" ;
.

<https://prez.dev/ManifestResourceRoles/CatalogueData>
    schema:description "Data for the container, usually a Catalogue, including the identity of it and each item fo content" ;
    schema:name "Catalogue Data" ;
.

<https://prez.dev/ManifestResourceRoles/CompleteCatalogueAndResourceLabels>
    schema:description "All the labels - possibly including names, descriptions & seeAlso links - for the Catalogue and Resource objects" ;
    schema:name "Complete Catalogue and Resource Labels" ;
.

<https://prez.dev/ManifestResourceRoles/ResourceData>
    schema:description "Data for the content of the container" ;
    schema:name "Resource Data" ;
.

schema:Organization
    schema:description "An organization such as a school, NGO, corporation, club, etc" ;
    schema:name "Organization" ;
.

schema:citation
    schema:description "A citation or reference to another creative work, such as another publication, web page, scholarly article, etc" ;
    schema:name "citation" ;
.

schema:codeRepository
    schema:description "Link to the repository where the un-compiled, human readable code and related code is located (SVN, GitHub, CodePlex)" ;
    schema:name "code repository" ;
.

schema:contributor
    schema:description "A secondary contributor to the CreativeWork or Event" ;
    schema:name "contributor" ;
.

schema:creator
    schema:description "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork" ;
    schema:name "creator" ;
.

schema:dateCreated
    schema:description "The date on which the CreativeWork was created or the item was added to a DataFeed" ;
    schema:name "date created" ;
.

schema:dateIssued
    schema:description "The date the ticket was issued" ;
    schema:name "date issued" ;
.

schema:dateModified
    schema:description "The date on which the CreativeWork was most recently modified or when the item's entry was modified within a DataFeed" ;
    schema:name "date modified" ;
.

schema:description
    schema:description "A description of the item" ;
    schema:name "description" ;
.

schema:isPartOf
    schema:description "Indicates an item or CreativeWork that this item, or CreativeWork (in some sense), is part of" ;
    schema:name "is part of" ;
.

schema:keywords
    schema:description "Keywords or tags used to describe some item. Multiple textual entries in a keywords list are typically delimited by commas, or by repeating the property" ;
    schema:name "keywords" ;
.

schema:name
    schema:description "The name of the item" ;
    schema:name "name" ;
.

schema:publisher
    schema:description "The publisher of the creative work" ;
    schema:name "publisher" ;
.

schema:status
    schema:description "The status of the study (enumerated)" ;
    schema:name "status" ;
.

schema:url
    schema:description "URL of the item" ;
    schema:name "url" ;
.

