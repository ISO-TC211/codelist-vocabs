PREFIX : <https://def.isotc211.org/common/CoverageContentTypeCode/>
PREFIX cs: <https://def.isotc211.org/common/CoverageContentTypeCode>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX status: <https://def.isotc211.org/RE/ItemStatus/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:auxilliaryInformation
    a skos:Concept ;
    skos:definition "data, usually a physical measurement, used to support the calculation of the primary physicalMeasurement coverages in the dataset (e.g. grid of aerosol optical thickness used in the calculation of a sea surface temperature product)."@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "auxilliary information"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:coordinate
    a skos:Concept ;
    skos:definition "data used to provide coordinate axis values"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "coordinate"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:image
    a skos:Concept ;
    skos:definition "meaningful numerical representation of a physical parameter that is not the actual value of the physical parameter"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "image"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:modelResult
    a skos:Concept ;
    skos:definition "resources with values that are calculated using a model rather than being observed or calculated from observations."@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "model result"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:physicalMeasurement
    a skos:Concept ;
    skos:definition "value in physical units of the quantity being measured"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "physical measurement"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:qualityInformation
    a skos:Concept ;
    skos:definition "data used to characterize the quality of the physicalMeasurement coverages in the dataset. Typically included in a gmi:QE_CoverageResult."@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "quality information"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:referenceInformation
    a skos:Concept ;
    skos:definition "reference information used to support the calculation or use of the physicalMeasurement coverages in the dataset (e.g. grids of latitude/longitude used to geolocate the physical measurements)."@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "reference information"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:thematicClassification
    a skos:Concept ;
    skos:definition "code value with no quantitative meaning, used to represent a physical quantity"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "thematic classification"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

cs:
    a skos:ConceptScheme ;
    skos:definition "specific type of information represented in the cell"@en ;
    skos:hasTopConcept
        :auxilliaryInformation ,
        :coordinate ,
        :image ,
        :modelResult ,
        :physicalMeasurement ,
        :qualityInformation ,
        :referenceInformation ,
        :thematicClassification ;
    skos:historyNote "This version of this codeset was produced for the 2025 update of the 19115 standard" ;
    skos:prefLabel "Coverage Content Type Code"@en ;
    schema:creator <https://def.isotc211.org/org/tc211> ;
    schema:dateCreated "2025-11-19"^^xsd:date ;
    schema:dateIssued "2025-11-19"^^xsd:date ;
    schema:dateModified "2025-11-19"^^xsd:date ;
    schema:isPartOf <https://def.isotc211.org/common> ;
    schema:publisher <https://def.isotc211.org/org/tc211> ;
    schema:status status:experimental ;
    schema:version "ISO19115-1:2025" ;
.
