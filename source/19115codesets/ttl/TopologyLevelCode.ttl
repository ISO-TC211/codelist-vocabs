PREFIX : <https://def.isotc211.org/common/TopologyLevelCode/>
PREFIX cs: <https://def.isotc211.org/common/TopologyLevelCode>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX status: <https://def.isotc211.org/RE/ItemStatus/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:abstract
    a skos:Concept ;
    skos:definition "topological complex without any specified geometric realisation"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "abstract"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:fullPlanarGraph
    a skos:Concept ;
    skos:definition "2-dimensional topological complex that is planar. (A 2-dimensional topological complex is commonly called 'full topology' in a cartographic 2D environment.)"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "full planar graph"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:fullSurfaceGraph
    a skos:Concept ;
    skos:definition "2-dimensional topological complex that is isomorphic to a subset of a surface"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "full surface graph"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:fullTopology3D
    a skos:Concept ;
    skos:definition "complete coverage of a 3D Euclidean coordinate space"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "full topology3 d"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:geometryOnly
    a skos:Concept ;
    skos:definition "geometry objects without any additional structure which describes topology"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "geometry only"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:planarGraph
    a skos:Concept ;
    skos:definition "1-dimensional topological complex that is planar. (A planar graph is a graph that can be drawn in a plane in such a way that no two edges intersect except at a vertex.)"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "planar graph"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:surfaceGraph
    a skos:Concept ;
    skos:definition "1-dimensional topological complex that is isomorphic to a subset of a surface. (A geometric complex is isomorphic to a topological complex if their elements are in a one-to-one, dimensional-and boundary-preserving correspondence to one another.)"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "surface graph"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:topology1D
    a skos:Concept ;
    skos:definition "1-dimensional topological complex - commonly called 'chain-node' topology"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "topology1 d"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

:topology3D
    a skos:Concept ;
    skos:definition "3-dimensional topological complex. (A topological complex is a collection of topological primitives that are closed under the boundary operations.)"@en ;
    skos:historyNote "Originally presented in the XXXX standard codelist, reproduced unchanged here"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "topology3 d"@en ;
    skos:topConceptOf cs: ;
    schema:status status:original ;
.

cs:
    a skos:ConceptScheme ;
    skos:definition "degree of complexity of the spatial relationships"@en ;
    skos:hasTopConcept
        :abstract ,
        :fullPlanarGraph ,
        :fullSurfaceGraph ,
        :fullTopology3D ,
        :geometryOnly ,
        :planarGraph ,
        :surfaceGraph ,
        :topology1D ,
        :topology3D ;
    skos:historyNote "This version of this codeset was produced for the 2025 update of the 19115 standard" ;
    skos:prefLabel "Topology Level Code"@en ;
    schema:creator <https://def.isotc211.org/org/tc211> ;
    schema:dateCreated "2025-11-19"^^xsd:date ;
    schema:dateIssued "2025-11-19"^^xsd:date ;
    schema:dateModified "2025-11-19"^^xsd:date ;
    schema:isPartOf <https://def.isotc211.org/common> ;
    schema:publisher <https://def.isotc211.org/org/tc211> ;
    schema:status status:experimental ;
    prov:wasDerivedFrom <https://def.isotc211.org/common/standard/iso-awi-19115-1> ;
.
